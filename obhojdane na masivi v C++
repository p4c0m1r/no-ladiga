#include <iostream>
using namespace std;
int MegaSlojnaZadacha()
{
	int arr[3][3] = { 2, 0, 7, 9, 1, 3, 4, 8, 2 }, sum = 0;
	for (int i = 0; i < 3; i++)
	{
		sum = 0;
		for (int j = 0; j < 3; j++)
		{
			cout << arr[i][j] << " ";
			sum += arr[i][j];
		}
		cout << sum << endl;
	}
	return 0;
	
}
int MegaSlojnaZadacha2()
{
	int arr[3][3] = { 2, 0, 7, 9, 1, 3, 4, 8, 2 }, sum = 0, n = 0;
	for (int i = 0; i < 3; i++)
	{

		for (int j = 0; j < 3; j++)
		{
			cout << arr[i][j] << " ";
		}
		cout << endl;
	}

	for (int i = 0; i < 3; i++)
	{

		for (int j = 0; j < 3; j++)
		{
			sum += arr[j][i];
		}
		cout << sum << " ";
	}
	return 0;
}
int MegaSlojnaZadacha3()
{
	int n, y;
	cin >> n >> y;
	int arr[10][20], sum = 0;
	for (int i = 0; i < n; i++)
	{
		for (int j = 0; j < y; j++)
		{
			cin >> arr[i][j];
		}
	}
	for (int i = 0; i < n; i++)
	{

		for (int j = 0; j < y; j++)
		{
			cout << arr[i][j] << " ";
		}
		cout << endl;
	}

	for (int i = 0; i < n; i++)
	{
		sum = 0;
		for (int j = 0; j < y; j++)
		{
			sum += arr[j][i];
		}
		cout << sum<< " ";
	}
	return 0;
}
int MegaSlojnaZadacha4()
{
	int n, y;
	cin >> n >> y;
	int arr[10][20], sum = 0;
	for (int i = 0; i < n; i++)
	{
		for (int j = 0; j < y; j++)
		{
			cin >> arr[i][j];
		}
	}

	for (int i = 0; i < n; i++)
	{
		for (int j = 0; j < y; j++)
		{
			cout << arr[i][j] << " ";
		}
		cout << endl;
	}

}
int main()
{
	MegaSlojnaZadacha3();
}
